ts" minOccurs="0"/> <xsd:element type="CourseList_t" name="Courses" minOccurs="0"/> <xsd:element type="AbstractSource_t" name="Author" minOccurs="0"/> -<xsd:element type="Extensions_t" name="Extensions" minOccurs="0"> -<xsd:annotation> <xsd:documentation>You can extend Training Center by adding your own elements from another schema here.</xsd:documentation> </xsd:annotation> </xsd:element> </xsd:sequence> </xsd:complexType> -<xsd:complexType name="Folders_t"> -<xsd:sequence> <xsd:element type="History_t" name="History" minOccurs="0"/> <xsd:element type="Workouts_t" name="Workouts" minOccurs="0"/> <xsd:element type="Courses_t" name="Courses" minOccurs="0"/> </xsd:sequence> </xsd:complexType> -<xsd:complexType name="ActivityList_t"> -<xsd:sequence> <xsd:element type="Activity_t" name="Activity" minOccurs="0" maxOccurs="unbounded"/> <xsd:element type="MultiSportSession_t" name="MultiSportSession" minOccurs="0" maxOccurs="unbounded"/> </xsd:sequence> </xsd:complexType> -<xsd:complexType name="WorkoutList_t"> -<xsd:sequence> -<xsd:element type="Workout_t" name="Workout" minOccurs="0" maxOccurs="unbounded"> -<xsd:annotation> <xsd:documentation> The StepId should be unique within a workout and should not exceed 20. This restricts the number of steps in a workout to 20. </xsd:documentation> </xsd:annotation> -<xsd:unique name="StepIdMustBeUnique"> <xsd:selector xpath=".//*"/> <xsd:field xpath="tc2:StepId"/> </xsd:unique> </xsd:element> </xsd:sequence> </xsd:complexType> -<xsd:complexType name="CourseList_t"> -<xsd:sequence> <xsd:element type="Course_t" name="Course" minOccurs="0" maxOccurs="unbounded"/> </xsd:sequence> </xsd:complexType> -<xsd:complexType name="History_t"> -<xsd:sequence> <xsd:element type="HistoryFolder_t" name="Running"/> <xsd:element type="HistoryFolder_t" name="Biking"/> <xsd:element type="HistoryFolder_t" name="Other"/> <xsd:element type="MultiSportFolder_t" name="MultiSport"/> -<xsd:element type="Extensions_t" name="Extensions" minOccurs="0"> -<xsd:an